BODY {
	MARGIN-TOP: 2px;
	BACKGROUND-COLOR: #FFFFFF;
	
}

TD { 

	FONT-SIZE: 8pt; COLOR: #000000; FONT-FAMILY: verdana, arial, sans-serif

}

TD.SectionNav {
	
	BORDER-STYLE: solid;
	BORDER-WIDTH: 1px;
	BORDER-COLOR:#CCCCCC;
	WHITE-SPACE: nowrap;
	FONT-SIZE: 12pt; COLOR: #000000; FONT-VARIANT: small-caps; FONT-WEIGHT: bold;	FONT-FAMILY: verdana, arial, sans-serif
	
}

TD.SectionNav:hover {
	
	/*BACKGROUND-IMAGE: url('images/menu_gray.gif');*/
	FONT-SIZE: 12pt; COLOR: #000000; FONT-VARIANT: small-caps; FONT-WEIGHT: bold;	FONT-FAMILY: verdana, arial, sans-serif
	
}

TD.SectionText {
	
	BORDER-STYLE: solid;
	BORDER-WIDTH: 1px;
	BORDER-COLOR:#CCCCCC;
	
}

TD.SectionNav2 {
	
	BORDER-STYLE: solid;
	BORDER-WIDTH: 1px;
	BORDER-COLOR:#000000;
	WHITE-SPACE: nowrap;
	FONT-SIZE: 12pt; COLOR: #000000; FONT-VARIANT: small-caps; FONT-WEIGHT: bold;	FONT-FAMILY: verdana, arial, sans-serif
	
}

TD.SectionNav2:hover {
	
	/*BACKGROUND-IMAGE: url('images/menu_gray.gif');*/
	FONT-SIZE: 12pt; COLOR: #000000; FONT-VARIANT: small-caps; FONT-WEIGHT: bold;	FONT-FAMILY: verdana, arial, sans-serif
	
}

TD.SectionText2 {
	
	BORDER-STYLE: solid;
	BORDER-WIDTH: 1px;
	BORDER-COLOR:#CCCCCC;
	
}

TD.BorderRight {
	
	border-right: 1px solid gray;
	
}

P { 

	FONT-SIZE: 10pt; COLOR: #000000; FONT-FAMILY: verdana, arial, sans-serif

}

A {

	FONT-SIZE: 8pt; COLOR: #B2B2B2; TEXT-DECORATION: none

}

A:link {

	COLOR: #336699; FONT-SIZE: 8pt; TEXT-DECORATION: none

}

A:visited {

	COLOR: #336699; FONT-SIZE: 8pt; TEXT-DECORATION: none

}

A:active {

	COLOR: #3333FF; FONT-SIZE: 8pt; TEXT-DECORATION: none

}

A:hover {

	COLOR: #3333FF; FONT-SIZE: 8pt; TEXT-DECORATION: underline

}

A.Nav {

	FONT-SIZE: 10pt; COLOR: #B2B2B2; TEXT-DECORATION: none

}

A.Nav:link {

	COLOR: #336699; FONT-SIZE: 10pt; TEXT-DECORATION: none

}

A.Nav:visited {

	COLOR: #336699; FONT-SIZE: 10pt; TEXT-DECORATION: none

}

A.Nav:active {

	COLOR: #3333FF; FONT-SIZE: 10pt; TEXT-DECORATION: none

}

A.Nav:hover {

	COLOR: #3333FF; FONT-SIZE: 10pt; TEXT-DECORATION: underline

}

#dhtmltooltip {
	
	position: absolute;
	width: 150px;
	border: 2px solid black;
	padding: 2px;
	background-color: lightyellow;
	visibility: hidden;
	z-index: 100;
	/*Remove below line to remove shadow. Below line should always appear last within this CSS*/
	filter: progid:DXImageTransform.Microsoft.Shadow(color=gray,direction=135);
	
}